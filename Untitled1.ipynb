    "def wildcard(words):\n",
    "    ws = sortedset()\n",
    "    for w in words:\n",
    "        for i in range(len(w)-1):\n",
    "            bi = w[i:i+2]\n",
    "            if bi not in bigrams.keys():\n",
    "                return \"not there\"\n",
    "            elif len(ws)==0:\n",
    "                ws = bigrams[bi]\n",
    "                continue\n",
    "            else:\n",
    "                ws = set.intersection(set(bigrams[bi]), ws)\n",
    "    final = sortedset()\n",
    "    for i in words:\n",
    "        e, s = i.split('$')\n",
    "        for i in ws:\n",
    "            if i.startswith(s) and i.endswith(e):\n",
    "                final.add(i)\n",
    "    print(final)\n",
